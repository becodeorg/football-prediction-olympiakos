{
  "version": "2.0",
  "functionTimeout": "00:15:00",
  "extensions": {
    "http": {
      "routePrefix": "api"
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true
      }
    },
    "logLevel": {
      "default": "Trace",  
      "Host.Results": "Trace",
      "Function": "Trace", 
      "Host.Aggregator": "Trace", 
      "Function.Python.Worker": "Trace"
    }
  }
}